{
  "name": "agentlaunch-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the AgentLaunch platform â€” create AI agent tokens, query market data, and generate handoff links",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "dist/**/*.map",
    "!dist/__tests__",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').writeFileSync('dist/index.cjs', \\\"'use strict';\\\\nconst m = require('./index.js');\\\\nmodule.exports = m;\\\\n\\\")\"",
    "build:watch": "tsc --watch",
    "test": "tsc --noEmit && tsc && node --test dist/__tests__/*.js",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agent-launch",
    "agentverse",
    "fetch-ai",
    "ai-agent",
    "token-launch",
    "sdk"
  ],
  "author": "Agent-Launch",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-launch/sdk"
  },
  "homepage": "https://agent-launch.ai",
  "bugs": {
    "url": "https://github.com/agent-launch/sdk/issues"
  }
}
